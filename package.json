{
    "name": "innovation_project",
    "productName": "innovation_project",
    "version": "1.0.0",
    "description": "My Electron application description",
    "main": "./.webpack/main",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts ."
    },
    "keywords": [],
    "author": {
        "name": "didilehbal",
        "email": "habilmehdi96@gmail.com"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [{
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "innovation_project"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {
                        "options": {}
                    }
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [{
                                "html": "./public/index.html",
                                "js": "./src/renderer.tsx",
                                "name": "home"
                            }]
                        }
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.9.0",
        "@electron-forge/cli": "^6.0.0-beta.52",
        "@electron-forge/maker-deb": "6.0.0-beta.49",
        "@electron-forge/maker-rpm": "6.0.0-beta.49",
        "@electron-forge/maker-squirrel": "6.0.0-beta.49",
        "@electron-forge/maker-zip": "6.0.0-beta.49",
        "@electron-forge/plugin-webpack": "6.0.0-beta.49",
        "@hot-loader/react-dom": "^16.11.0",
        "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
        "@svgr/webpack": "^5.4.0",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.4.2",
        "electron": "^9.0.2",
        "eslint": "^6.8.0",
        "eslint-plugin-import": "^2.20.0",
        "fork-ts-checker-webpack-plugin": "^3.1.1",
        "node-loader": "^0.6.0",
        "node-sass": "^4.13.1",
        "react-hot-loader": "^4.12.19",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.0"
    },
    "dependencies": {
        "@ant-design/compatible": "^1.0.2",
        "@nestjsx/crud-request": "^4.6.2",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.3",
        "antd": "^4.2.4",
        "antd-img-crop": "^3.3.0",
        "axios": "^0.21.1",
        "electron-devtools-installer": "^3.0.0",
        "electron-squirrel-startup": "^1.0.0",
        "moment": "^2.26.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-highlight-words": "^0.16.0",
        "react-keyed-file-browser": "^1.9.0",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.3.1",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "rtcmulticonnection": "^3.7.0"
    }
}